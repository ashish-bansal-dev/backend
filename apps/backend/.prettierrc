{
  "semi": false,
  "singleQuote": false,
  "tabWidth": 2,
  "printWidth": 80,
  "trailingComma": "es5",
  "arrowParens": "always",
    "importOrder": [
        "^@medusajs/(.*)$",
        "^@mercurjs/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
        "@trivago/prettier-plugin-sort-imports"
    ],
    "importOrderParserPlugins": [
        "typescript",
        "decorators-legacy"
    ]
}